{
  "name": "glacier",
  "version": "1.0.0",
  "description": "A JS like language for The TI84PCE",
  "main": "main.js",
  "directories": {
    "test": "tests"
  },
  "type": "module",
  "scripts": {
    "env": "echo \"export DEVDIR=~&&export DEBUG=true\"",
    "start": "node ./main.js",
    "log": "node ./logviewer.cjs -list ",
    "build": "cd $DEVDIR/ICEScript&& npm run clean; npm run compileVisitor && npm test",
    "buildDbg": "cd $DEVDIR/ICEScript&& npm run clean; npm run compileDbg && npm test",
    "test": "cd $DEVDIR/ICEScript&&tsc ./src/llvm.ts&&npm start ./tests/fakeHome.gs",
    "compileVisitor": "antlr4 -o ./antlr/parsers -lib ./antlr/grammars -visitor -Dlanguage=JavaScript  ./antlr/grammars/ICEScript",
    "clean": "rm ./antlr/* && rm -r ./antlr/parsers/antlr           ",
    "compileDbg": "antlr4 -XdbgST -o ./antlr/parsers -lib ./antlr/grammars -visitor -Dlanguage=JavaScript  ./antlr/grammars/ICEScript"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/DarkSystemGit/ICEScript"
  },
  "keywords": [
    "ICEScript",
    "JS",
    "TI84PCE"
  ],
  "author": "darksystem",
  "license": "MIT",
  "devDependencies": {
    "antlr4": "^4.13.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "yaml": "^2.3.4"
  }
}
