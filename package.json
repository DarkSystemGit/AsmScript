{
  "name": "glacier",
  "version": "1.0.0",
  "description": "A JS like language for The TI84PCE",
  "main": "main.js",
  "directories": {
    "test": "tests"
  },
  "type": "module",
  "scripts": {
    "env": "echo \"export DEVDIR=~/Glacier&&export DEBUG=true\"",
    "start": "node ./src/parser/parser.js",
    "log": "node ./logviewer.cjs -list ",
    "build": "node ./scripts/createVisitor&&npm start ",
    "install": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/DarkSystemGit/Glacier"
  },
  "keywords": [
    "Glacier",
    "JS",
    "TI84PCE"
  ],
  "author": "darksystem",
  "license": "MIT",
  "devDependencies": {
    "antlr4": "^4.13.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@prettier/sync": "^0.5.1",
    "@swc/core": "^1.3.102",
    "import-sync": "^2.2.0",
    "inquirer": "^9.2.12",
    "json5": "^2.2.3",
    "woodpile": "^0.0.5",
    "yaml": "^2.3.4"
  }
}
