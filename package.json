{
  "name": "icescript",
  "version": "1.0.0",
  "description": "A JS like language for The TI84PCE",
  "main": "main.js",
  "directories": {
    "test": "tests"
  },
  "type":"module",
  "scripts": {
    "codespaces":"export DEVDIR=/workspaces",
    "start":"node ./main.js",
    "build": "cd $DEVDIR/ICEScript&& npm run clean; npm run compileVisitor && npm start ./tests/fakeHome.is",
    "test":"cd $DEVDIR/ICEScript&&npm start ./tests/fakeHome.is",
    "compileVisitor": "antlr4 -visitor -o ./antlr/parsers -lib ./antlr/grammars -Dlanguage=JavaScript ./antlr/grammars/TiB && antlr4 -o ./antlr/parsers -lib ./antlr/grammars -visitor -Dlanguage=JavaScript  ./antlr/grammars/ICEScript",
"clean":"rm ./antlr/* && rm -r ./antlr/parsers/antlr           ",
"compile":"antlr4  -o ./antlr/parsers -lib ./antlr/grammars -Dlanguage=JavaScript ./antlr/grammars/TiB && antlr4 -o ./antlr/parsers -lib ./antlr/grammars  -Dlanguage=JavaScript  ./antlr/grammars/ICEScript"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/DarkSystemGit/ICEScript"
  },
  "keywords": [
    "ICEScript",
    "JS",
    "TI84"
  ],
  "author": "darksystem",
  "license": "MIT",
  "dependencies": {
    "antlr4": "^4.13.1"
  }
}
